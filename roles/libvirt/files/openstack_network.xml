<network>
  <name>openstack</name>
  <forward mode="bridge"/>
  <bridge name="openstack"/>
  <virtualport type="openvswitch"/>
  <portgroup name="internal">
    <vlan trunk="yes">
      <tag id="100" nativeMode="untagged"/>
      <tag id="101" />
      <tag id="102" />
    </vlan>
  </portgroup>
  <portgroup name="stirage">
    <vlan>
      <tag id="103"/>
    </vlan>
  </portgroup>
  <portgroup name="external">
    <vlan>
      <tag id="200"/>
    </vlan>
  </portgroup>
  <portgroup name="floating">
    <vlan>
      <tag id="201"/>
    </vlan>
  </portgroup>
</network>
